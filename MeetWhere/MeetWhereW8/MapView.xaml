<UserControl
    x:Class="MeetWhere.MapView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >

    <Canvas x:Name="LayoutRoot">
        <Image x:Name="viewBackground" />
        <Canvas x:Name="viewChildren" />
        <Ellipse x:Name="scanLoc" Visibility="Collapsed" Fill="Magenta" Width="5" Height="5" />
        <Ellipse x:Name="currentLoc" Visibility="Collapsed" Width="500" Height="500"
                 Fill="#40ffffff" Stroke="{StaticResource ListBoxItemSelectedPointerOverBackgroundThemeBrush}" />
    </Canvas>
</UserControl>
