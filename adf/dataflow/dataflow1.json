{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DailyRawTotalsBlob",
						"type": "DatasetReference"
					},
					"name": "DailyRawTotalsBlob"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ADL",
						"type": "DatasetReference"
					},
					"name": "sink3"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          NumHits as long,",
				"          AbsoluteACores as double,",
				"          AbsoluteDCores as double,",
				"          AbsoluteACoresLinux as double,",
				"          AbsoluteDCoresLinux as double,",
				"          TotalCores as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DailyRawTotalsBlob",
				"DailyRawTotalsBlob sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink3"
			]
		}
	}
}